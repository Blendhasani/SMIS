@model IEnumerable<WebApplication5.Models.Fakulteti>

@{
	ViewData["Title"] = "Fakultetet";
}
<div class="bg-light p-5 rounded">

<h1 class="mt-3 mb-3">Fakultetet</h1>



<table class="table">
	<thead>
		<tr class="text-center">
			<th>
				@Html.DisplayNameFor(model => model.Emri)
			</th>
		

		

			<th></th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
				<tr class="text-center align-middle">
					<td>
					@Html.DisplayFor(modelItem => item.Emri)
					</td>
			


						<td class="align-middle" style="display:flex; flex-direction:column;">


					<a asp-action="Students" class="btn btn-success" asp-controller="StudentTeachers" asp-route-id="@item.Id">Studentët</a>



						</td>
				</tr>
		}
	</tbody>
</table>
</div>